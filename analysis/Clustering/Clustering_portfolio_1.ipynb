    "#portfolio 1 com labelencoder: p1enc\n",
    "p1enc=pd.DataFrame()\n",
    "categorical_cols = ['de_natureza_juridica', 'sg_uf', 'de_ramo']\n",
    "# %*%*%*%%*%*%*%%*%*%*%%*%*%*%%*%*%*%%*%*%*%\n",
